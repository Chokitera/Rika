@{
    ViewData["Title"] = "Criar Contatos";
}

<div class="text-center">

    <h1 class="display-4">Editar Contato</h1>

</div>

<form>
    <div class="form-group">
        <label for="nome">Nome Complmeto</label>
        <input type="text" class="form-control" placeholder="Nome Completo">
    </div>
    <div class="form-group">
        <label for="email">E-mail</label>
        <input type="email" class="form-control" placeholder="E-mail">
    </div>
    <div class="form-group">
        <label for="celular">E-mail</label>
        <input type="tel" class="form-control" placeholder="Número de Celular">
    </div>
    <br/>
    <button type="submit" class="btn btn-primary">Alterar</button>
    <a class="btn btn-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>